# A small ttd (Token- and Type-Definitions) file defining the mathematical regular expression notation

# TOKENS
lit := "[^01()+*\\]+\\[^]"
0   := "0"
1   := "1"
(   := "\("
)   := "\)"
+   := "\+"
*   := "\*"

# TYPES
re  ::= re2 + re | re2
re2 ::= re3 re2  | re3
re3 ::= re3 *    | re4
re4 ::= 0 | 1    | lit | ( re )
